{
    "name": "whatwedo/crud-history-bundle",
    "type": "symfony-bundle",
    "description": "Extends whatwedo crud bundle with history view from auditor-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "whatwedo GmbH",
            "email": "welove@whatwedo.ch"
        }
    ],
    "require": {
        "php":                      ">=8.1",
        "whatwedo/table-bundle": "dev-1.0-dev as 1.0.0",
        "whatwedo/core-bundle": "dev-1.0-dev as 1.0.0",
        "whatwedo/crud-bundle": "dev-feature/v1.0-dev-security-proposal as 1.0.0",
        "whatwedo/search-bundle": "dev-3.0-dev",
        "damienharper/auditor": "dev-master as 1.3.1",
        "damienharper/auditor-bundle": "dev-master",
        "symfony/security-bundle": "6.1.x-dev"

    },
    "require-dev": {
        "symfony/phpunit-bridge": "^v6.0",
        "zenstruck/foundry": "1.x-dev",
        "symfony/yaml": "^v6.0",
        "whatwedo/php-coding-standard": "dev-develop",
        "symfony/test-pack": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "whatwedo\\CrudHistoryBundle\\": "src/",
            "whatwedo\\CrudHistoryBundle\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "twbs/icons",
                "version": "1.6.1",
                "source": {
                    "url": "https://github.com/twbs/icons",
                    "type": "git",
                    "reference": "tags/v1.6.1"
                }
            }
        }
    ]
}
